  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "font-family-name-quotes": "always-unless-keyword",
      "function-url-quotes": "always",
      "number-leading-zero": "always",
      "length-zero-no-unit": true,
      "declaration-block-no-duplicate-properties": null,
      "selector-pseudo-element-colon-notation": "single",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ]
    }
  },
  "scripts": {
    "start": "set NODE_ENV=development; babel-node tools/run start",
    "test": "babel-node ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS",
    "test:watch": "babel-node ./node_modules/karma/bin/karma start --single-run=false --browsers PhantomJS",
    "lint": "eslint src tools",
    "stylelint": "stylelint \"src/components/**/*.scss\"",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build:styles": "babel-node tools/run styles",
    "build:dll": "npm run clean:all; babel-node tools/run vendors"
